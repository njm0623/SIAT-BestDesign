<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="designer">
	<insert id="designerInsert" parameterType="design">
		<![CDATA[
			INSERT INTO
			DESIGNER(designerId,designerPwd,designerName,designerEmail,designerBirth,designerPhone)
			VALUES(#{designerId},#{designerPwd},#{designerName},#{designerEmail},#{designerBirth},#{designerPhone})
			]]>
	</insert>
	<select id="designerLogin" parameterType="design" resultType="design">
		<![CDATA[
			SELECT *
			FROM DESIGNER
			WHERE designerId = #{designerId}
		]]>
	</select>
</mapper>